<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="airline_bill_counter_view_tree" model="ir.ui.view">
            <field name="name">airline_bill_counter_view_tree</field>
            <field name="model">airline.bill.counter</field>
            <field name="arch" type="xml">
                <tree string="Airline Bill Counter" editable="top">
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="action_airline_bill_counter" model="ir.actions.act_window">
            <field name="name">Airline Bill Counter</field>
            <field name="res_model">airline.bill.counter</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>