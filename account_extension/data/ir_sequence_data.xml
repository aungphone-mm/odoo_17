<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Payment -->
        <record id="seq_account_cashbook_payment" model="ir.sequence">
            <field name="name">Account Cashbook Payment Sequence</field>
            <field name="code">account.cashbook.payment</field>
            <field name="prefix">PAY/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Receive -->
        <record id="seq_account_cashbook_receive" model="ir.sequence">
            <field name="name">Account Cashbook Receive Sequence</field>
            <field name="code">account.cashbook.receive</field>
            <field name="prefix">REC/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>